		renesas,#ch = <0>;
	};

	vpc0xy: vcp0@fe960380 {
		compatible = "renesas,vpc0xy";
		reg = <0 0xfe960380 0 0x4>;
		/* clocks = <&mstp1_clks R8A77470_CLK_VPC0>; */
