	mmc: mmc@ee300000 {
		compatible = "renesas,sdhi-r8a77470";
		reg = <0 0xee300000 0 0x2000>;
		interrupts = <0 166 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&mstp3_clks R8A77470_CLK_SDHI1>;
